<?xml version="1.0" ?>
<Component>
    <Name>Shield_ULN2003_Motor_Shield</Name>
    <Description>Motor shield driver</Description>

    <Version>1.0.0</Version>
    <KeyWord>Motor, Shield, PWM, ULN2003, CoX</KeyWord>
    
    <Filter>
        <Interface>GPIO</Interface>
        <Interface>TIMER</Interface>
        <CoXBased>true</CoXBased>
    </Filter>

<!-- Categorys example for all components, Layer.Cate, Layer.Cate.SubCate can be empty  
    <Categorys>
        <Layer name="Middleware">
            <Cate name="Audio">
                <SubCate name="MP3" />
            </Cate>
        </Layer>
    </Categorys> 
-->
    <Categorys>
        <Layer name="Driver">
            <Cate name="Driver" />
        </Layer>
    </Categorys> 
    
    <SupportDevices>
        <Device>Motor_Shield</Device>
    </SupportDevices>
  
  
    <Files>
        <File>$Driver_ROOT$\lib\*</File>
    </Files>
    
    <CoIDEFiles>
        <File>PROJECT_ROOT$\CoX\Shield\TinkerKIT\Motor_Shield\lib\MotorShield.h</File>
        <File>PROJECT_ROOT$\CoX\Shield\TinkerKIT\Motor_Shield\lib\MotorShield.c</File>
    </CoIDEFiles>
    
    <DependentComponentInterfaceTag>
        <Tag>CoX.GPIO</Tag>
        <Tag>CoX.SysCtl</Tag>
        <Tag>CoX.Timer</Tag>
    </DependentComponentInterfaceTag>
    
    <SupportMCUs>
        <MCU></MCU>
    </SupportMCUs>
    
    <SupportMCUSeries>
        <Series>All</Series>
    </SupportMCUSeries>
    
    <Doc>$Driver_ROOT$\doc\</Doc>
  
</Component>